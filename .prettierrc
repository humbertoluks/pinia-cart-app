{
  "semi": true,                     
  "singleQuote": true,              
  "printWidth": 80,                 
  "tabWidth": 4,                    
  "trailingComma": "es5",           
  "bracketSpacing": true,           
  "vueIndentScriptAndStyle": true,  
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "quoteProps": "consistent", 
        "singleQuote": true         
      }
    }
  ]
}